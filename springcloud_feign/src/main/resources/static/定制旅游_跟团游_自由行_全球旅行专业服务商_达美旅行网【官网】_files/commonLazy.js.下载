define(["lazyload","cookie"],function(e){var s=void $("img.lazy").lazyload({event:"scroll mouseover",threshold:200,effect:"fadeIn"}),l={update:function(){$("img.lazy").lazyload({event:"scroll",threshold:200,effect:"fadeIn"})}},a={update:function(){var e,s=$(".more_expand"),l=[];if(e=$.cookie("moreshowus"),"hasExpand"==$.cookie("moreus")){var a=$("span.show-collapse");$(".more-filterg").slideDown("slow"),a.children("i").addClass("rotate"),a.find("em").text("收起")}if(e){var o=e.split(",");s.each(function(e,s){"2"==o[e]&&($(s).prev("ul").removeClass("collapse").addClass("expand"),$(s).text("[收起]"))})}s.each(function(e,s){$(s).prev("ul").find(".filterItem").length<=8&&$(s).hide(),$(s).prev("ul").hasClass("filter-tab-list")&&$(s).prev("ul").find(".taps").height()>100&&$(s).show(),$(s).on("click",function(s){s.preventDefault();var a=$(this);a.prev("ul").hasClass("collapse")?(l[e]="2",$.cookie("moreshowus",l,2),a.prev("ul").removeClass("collapse").addClass("expand"),a.text("[收起]")):(l[e]="1",$.cookie("moreshowus",l,1),a.prev("ul").addClass("collapse").removeClass("expand"),a.text("[更多]"))})}),$("span.show-collapse").on("click",function(e){e.preventDefault();var s=$(this),l=s.children("span").data("text");$(".more-filterg").is(":hidden")?($(".more-filterg").slideDown("slow"),s.find("em").text("收起"),s.children("i").addClass("rotate"),$.cookie("moreus","hasExpand")):($.cookie("moreus",null),$(".more-filterg").slideUp("slow"),l?s.find("em").text("展开"):s.children("span").text("展开"),s.children("i").removeClass("rotate"))}),$("span.more_choose").on("click",function(e){e.preventDefault();var s=$(this),l=s.siblings("ul").find(".is_morechoose"),a=s.prev(".more_expand");s.siblings("ul").hasClass("collapse")?(s.siblings("ul").removeClass("collapse").addClass("expand"),l.removeClass("hide")):l.is(":hidden")?l.removeClass("hide"):l.addClass("hide"),a.is(":hidden")||a.text("[收起]")})}};return a.update(),{loadLazyimg:s,dynamicLazyimg:l,loadfilter:a}});