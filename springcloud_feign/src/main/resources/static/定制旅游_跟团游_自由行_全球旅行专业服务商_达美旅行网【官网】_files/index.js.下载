require(["/pc/js/require/config.js?v="+require_ver.version],function(){require(["carousel","commonLazy","tabs","commonTips"],function(e,s,i,n){function a(){$("div.box_b a").eq(t).addClass("cur").siblings().removeClass("cur"),$("div.sildebox >div").eq(t).removeClass("hide").siblings().addClass("hide"),$("div.case_intro >li").eq(t).show().siblings().hide(),3==++t&&(t=0)}$("#slideshow").Carousels({auto:6e3}),$("div.search form input").hover(function(e){""==$("input[name=key]").val()&&$(".search-list").show(),$(".search input").focus()},function(){}),$("div.search").hover(function(e){},function(){$(".search-list").hide(),$(".search input").blur()}),$("div.search input").keyup(function(e){var s=$(this);""!=s.val()?s.parents(".search-box").find(".search-list").hide():s.parents(".search-box").find(".search-list").show()}),$("div.search .search-btn").on("click",function(e){e.stopPropagation(),$("div.search input").val()?$("#key_search").submit():n.Walert({closeBtn:!1,contentHtml:"请输入你想要搜索的关键词"})}),$("#key_search").keypress(function(e){if(13==e.which&&""==$("div.search input").val())return n.Walert({closeBtn:!1,contentHtml:"请输入你想要搜索的关键词"}),!1}),$(".tagList span").on("click",function(e){e.preventDefault();var s="/search.html?key="+$(this).text();window.open(s,"_blank")}),$(".search-list li").on("click",function(e){e.preventDefault();var s="/search.html?key="+$(this).children("a").text();window.open(s,"_blank")}),$("#hot-recommend").Tabs(),$("#hot-topics").Tabs();var t=0,r=setInterval(a,2e3);$(".box_b a").mouseover(function(e){e.preventDefault(),clearInterval(r);var s=$(this).index();$(".box_b a").eq(s).addClass("cur").siblings().removeClass("cur"),$(".sildebox >div").eq(s).removeClass("hide").siblings().addClass("hide"),$("div.case_intro >li").eq(s).show().siblings().hide()}).mouseleave(function(){r=setInterval(a,2e3)})})});